<%= form_for @song do |f| %>
    <%= f.label :name, 'Song Name' %><br>
    <%= f.text_field :name %><br>

    <%= f.label :artist_id, 'Artist ID' %><br>
    <%= f.text_area :artist_id %><br>

    <%= f.label :genre_id, 'Gemre ID' %><br>
    <%= f.text_area :genre_id %><br>

    <%= f.submit %>
<% end %>